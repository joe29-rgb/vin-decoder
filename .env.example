# DO NOT PUT REAL SECRETS HERE. Copy to .env and fill values locally.
NODE_ENV=development
PORT=10000
LOG_LEVEL=info

# GoHighLevel OAuth
GHL_CLIENT_ID=your_ghl_client_id
GHL_CLIENT_SECRET=your_ghl_client_secret
GHL_REDIRECT_URI=https://your-domain.com/api/ghl/oauth/callback
GHL_WEBHOOK_SECRET=replace_with_random_string

# GoHighLevel Legacy (optional)
GHL_API_KEY=your_ghl_api_key
GHL_BASE_URL=https://services.leadconnectorhq.com/v1
GHL_LOCATION_ID=your_location_id

# VIN Audit
VINAUDIT_API_KEY=your_vinaudit_api_key

# CARFAX
CARFAX_API_KEY=your_carfax_api_key

# Geolocation (REQUIRED for competitor scraper radius search)
# Get your API key at: https://console.cloud.google.com/apis/credentials
# Enable: Geocoding API, Maps JavaScript API
GOOGLE_MAPS_API_KEY=your_google_maps_api_key
GEOCODING_API_KEY=your_geocoding_api_key

# Supabase (NO SERVICE ROLE KEY HERE)
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_ANON_KEY=your_public_anon_key

# Apify API for AutoTrader/CarGurus scraping
APIFY_API_TOKEN=your_apify_api_token
APIFY_AUTOTRADER_ACTOR_ID=apify/autotrader-scraper
APIFY_CARGURUS_ACTOR_ID=apify/cargurus-scraper

# Canadian Black Book API (per dealership subscription)
CBB_API_KEY=your_cbb_api_key
CBB_API_URL=https://api.canadianblackbook.com/v1

# Optional tuning
RATE_LIMIT_WINDOW_MS=60000
RATE_LIMIT_MAX_REQUESTS=120
HTTP_TIMEOUT_MS=30000
RETRY_MAX_ATTEMPTS=3
RETRY_DELAY_MS=500
